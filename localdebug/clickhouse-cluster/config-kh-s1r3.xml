<clickhouse>
    <remote_servers>
        <statlogs2>
            <shard>
                <replica>
                    <host>kh-s1r1</host>
                    <port>9000</port>
                </replica>
                <replica>
                    <host>kh-s1r2</host>
                    <port>9000</port>
                </replica>
                <replica>
                    <host>kh-s1r3</host>
                    <port>9000</port>
                </replica>
            </shard>
            <shard>
                <replica>
                    <host>kh-s2r1</host>
                    <port>9000</port>
                </replica>
                <replica>
                    <host>kh-s2r2</host>
                    <port>9000</port>
                </replica>
                <replica>
                    <host>kh-s2r3</host>
                    <port>9000</port>
                </replica>
            </shard>
            <shard>
                <replica>
                    <host>kh-s3r1</host>
                    <port>9000</port>
                </replica>
                <replica>
                    <host>kh-s3r2</host>
                    <port>9000</port>
                </replica>
                <replica>
                    <host>kh-s3r3</host>
                    <port>9000</port>
                </replica>
            </shard>
        </statlogs2>
    </remote_servers>

    <macros>
        <shard>1</shard>
        <replica>3</replica>
    </macros>

    <zookeeper>
        <node>
            <host>zoo1</host>
            <port>2181</port>
        </node>
        <node>
            <host>zoo2</host>
            <port>2181</port>
        </node>
        <node>
            <host>zoo3</host>
            <port>2181</port>
        </node>
    </zookeeper>

    <prometheus>
        <endpoint>/metrics</endpoint>
        <port>9363</port>
        <metrics>true</metrics>
        <events>true</events>
        <asynchronous_metrics>true</asynchronous_metrics>
        <status_info>true</status_info>
    </prometheus>
</clickhouse>
