<clickhouse>
    <remote_servers>
        <local_test_cluster>
            <shard>
                <replica>
                    <host>aggregator</host>
                    <port>9000</port>
                </replica>
            </shard>
        </local_test_cluster>
    </remote_servers>
    <prometheus>
        <endpoint>/metrics</endpoint>
        <port>9363</port>
        <metrics>true</metrics>
        <events>true</events>
        <asynchronous_metrics>true</asynchronous_metrics>
        <status_info>true</status_info>
    </prometheus>
</clickhouse>
