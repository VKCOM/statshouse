#!/usr/bin/make -f
# See debhelper(7) (uncomment to enable)
# output every command that modifies files on the build system.
DH_VERBOSE = 1

# see EXAMPLES in dpkg-buildflags(1) and read /usr/share/dpkg/*
DPKG_EXPORT_BUILDFLAGS = 1
#include /usr/share/dpkg/default.mk

# Uncomment submodules if you run local build:
override_dh_auto_build:
	${MAKE} build-sh build-sh-metadata build-sh-api build-sh-ui build-sh-grafana build-grafana-ui

override_dh_install:
	mv target/statshouse-grafana-plugin target/gpx_vk-statshouse_linux_amd64
	dh_install

override_dh_installdocs:

%:
	dh $@
